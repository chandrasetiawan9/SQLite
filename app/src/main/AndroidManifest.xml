<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="chandra023.sqliteapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/img"
        android:label="SQLite-chandra023"
        android:roundIcon="@drawable/img"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="chandra023.sqliteapp.ContentActivity"></activity>
        <activity android:name="chandra023.sqliteapp.RegisterActivity" />
        <activity android:name="chandra023.sqliteapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>